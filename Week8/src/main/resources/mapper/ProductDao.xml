<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.week8.dao.ProductDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.example.week8.entity.ProductEntity" id="productMap">
        <result property="productId" column="product_id"/>
        <result property="productName" column="product_name"/>
        <result property="productImgPath" column="product_img_path"/>
        <result property="productInformation" column="product_information"/>
        <result property="productUnitName" column="product_unit_name"/>
        <result property="productBrandName" column="product_brand_name"/>
        <result property="createTimestamp" column="create_timestamp"/>
        <result property="updateTimestamp" column="update_timestamp"/>
    </resultMap>


</mapper>